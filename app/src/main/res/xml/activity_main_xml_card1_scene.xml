<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="400">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragLeft" />
        <OnClick />
        <OnSwipe
            motion:touchAnchorId="@+id/textView7"
            motion:dragDirection="dragLeft"
            motion:touchAnchorSide="right" />
        <OnClick motion:targetId="@+id/textView7" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:id="@+id/textView" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:id="@+id/textView"
            motion:layout_constraintStart_toEndOf="@+id/imageView3" />
        <Constraint
            android:id="@+id/textView12"
            motion:layout_constraintStart_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="45dp"
            android:visibility="invisible"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="10dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:id="@+id/imageView5" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:id="@+id/textView"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            android:layout_marginTop="5dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:layout_constraintStart_toStartOf="@+id/textView2"
            android:layout_marginTop="10dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="10dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView3"
            motion:layout_constraintStart_toEndOf="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView4"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintVertical_bias="1.0"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView5"
            motion:layout_constraintStart_toEndOf="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView6"
            motion:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintVertical_bias="1.0"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="150dp"
            android:layout_width="200dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:id="@+id/textView12"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/imageView8"
            motion:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            android:layout_marginStart="8dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView7"
            motion:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_width="20dp"
            android:layout_height="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"
            android:layout_marginStart="8dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView5"
            android:visibility="invisible" />
    </ConstraintSet>
</MotionScene>